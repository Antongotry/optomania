<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}" class="no-transition{% if body_class %} body-product{% endif %}{% if oct_theme is defined and oct_theme == 'dark' %} dark-theme{% else %} light-theme{% endif %}" data-oct-fonts="{{ main_active_font.data_attr }}">
<!--<![endif]-->
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>{{ title }}</title>
{% if (oct_promo_data.open_graph is defined and oct_promo_data.open_graph) and octOpenGraphs is not empty %}
{% for octOpenGraph in octOpenGraphs %}
{% if octOpenGraph.content is not empty %}
<meta property="{{ octOpenGraph.property }}" content="{{ octOpenGraph.content }}" />
{% endif %}
{% endfor %}
{% endif %}
{% if oct_theme_color is defined and oct_theme_color %}
<meta name="theme-color" content="{{ oct_theme_color }}" data-oct-theme-color="{{ oct_theme_color }}"/>
{% endif %}
{% if oct_promo_data.dark_theme is defined %}
    <script id="octDetectTheme">
        (function() {
            const darkThemeColor = 'rgb(29, 29, 29)';
            let lightThemeColor = 'rgb(255, 255, 255)';

            const metaThemeColor = document.querySelector('[name="theme-color"]');
            if (metaThemeColor && metaThemeColor.hasAttribute('data-oct-theme-color')) {
                lightThemeColor = metaThemeColor.getAttribute('data-oct-theme-color');
            }

            const storedTheme = localStorage.getItem('theme');
            const deviceTheme = window.matchMedia("(prefers-color-scheme: dark)").matches ? 'dark' : 'light';
            const adminTheme = '{{ oct_theme }}';

            let initialTheme;

            if (storedTheme) {
                initialTheme = storedTheme;
            } else if (adminTheme === 'auto') {
                initialTheme = deviceTheme;
            } else {
                initialTheme = adminTheme;
            }

            if (initialTheme) {
                document.documentElement.classList.remove('light-theme', 'dark-theme');
                document.documentElement.classList.add(`${initialTheme}-theme`);
                if (metaThemeColor) {
                    metaThemeColor.setAttribute('content', initialTheme === 'dark' ? darkThemeColor : lightThemeColor);
                }
            }
        })();
    </script>
{% endif %}
{% if oct_analytics_google_webmaster_code is defined and oct_analytics_google_webmaster_code is not empty %}
<meta name="google-site-verification" content="{{ oct_analytics_google_webmaster_code }}" />
{% endif %}
{% if oct_analytics_googletm_code is defined and oct_analytics_googletm_code is not empty %}
