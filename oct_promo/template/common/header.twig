                    <script>
                        function updateThemeButtonsVisibility() {
                            const currentTheme = document.documentElement.className.includes('dark-theme') ? 'dark' : 'light';
                            const lightButtons = document.querySelectorAll('[data-theme-toggle="light"]');
                            const darkButtons = document.querySelectorAll('[data-theme-toggle="dark"]');
                            
                            if (currentTheme === 'dark') {
                                lightButtons.forEach(btn => btn.style.display = 'none');
                                darkButtons.forEach(btn => btn.style.display = 'flex');
                            } else {
                                lightButtons.forEach(btn => btn.style.display = 'flex');
                                darkButtons.forEach(btn => btn.style.display = 'none');
                            }
                        }

                        updateThemeButtonsVisibility();

                        document.addEventListener('DOMContentLoaded', () => {
                            switchTheme();
                            
                            const themeButtons = document.querySelectorAll('[data-theme-toggle]');
                            themeButtons.forEach(button => {
                                button.addEventListener('click', () => {
                                    setTimeout(updateThemeButtonsVisibility, 50);
                                });
                            });
