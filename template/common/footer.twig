						</div>
					{% endif %}
					{% if oct_promo_data.payments.pp is defined and oct_promo_data.payments.pp %}
						<div class="pr-footer-item-payments-item br-4 bg-grey p-2">
							<img class="d-block img-fluid" src="{{ base}}catalog/view/theme/oct_promo/images/payments/paypal.svg" alt="PayPal" width="50" height="30" loading="lazy">
						</div>
					{% endif %}
					{% if oct_promo_data.payments.skrill is defined and oct_promo_data.payments.skrill %}
						<div class="pr-footer-item-payments-item br-4 bg-grey p-2">
							<img class="d-block img-fluid" src="{{ base}}catalog/view/theme/oct_promo/images/payments/skrill.svg" alt="Skrill" width="50" height="30" loading="lazy">
						</div>
					{% endif %}
					{% if oct_promo_data.payments.interkassa is defined and oct_promo_data.payments.interkassa %}
						<div class="pr-footer-item-payments-item br-4 bg-grey p-2">
							<img class="d-block img-fluid" src="{{ base}}catalog/view/theme/oct_promo/images/payments/interkassa.svg" alt="Interkassa" width="50" height="30" loading="lazy">
						</div>
