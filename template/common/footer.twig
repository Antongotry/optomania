<footer class="pr-footer mt-4">
	<div class="container-fluid container-xl">
		<div class="row gy-5">
			<div class="col-12 col-sm-6 col-lg-4">
				{% if logo %}
					<div class="mb-5">
						{% if oct_home is defined and oct_home %}
							<span><img class="pr-footer-logo d-block mx-auto mb-3 m-md-0" src="{{ logo }}" title="{{ name }}" alt="{{ name }}" {% if logo_width is defined and logo_width %} width="{{ logo_width }}" {% endif %} {% if logo_height is defined and logo_height %} height="{{ logo_height }}" {% endif %}/></span>
						{% else %}
							<a href="{{ home }}" title="{{ name }}">
								<img class="pr-footer-logo d-block mx-auto mb-3 m-md-0" src="{{ logo }}" title="{{ name }}" alt="{{ name }}" {% if logo_width is defined and logo_width %} width="{{ logo_width }}" {% endif %} {% if logo_height is defined and logo_height %} height="{{ logo_height }}" {% endif %}/>
							</a>
						{% endif %}
					</div>
				{% endif %}
				{% if main_address is defined and main_address %}
					<div class="pr-footer-item pr-footer-item-address">
						<div class="pr-footer-item-title white-text fw-700 d-flex align-items-center gap-2 mb-2">
							<svg xmlns="http://www.w3.org/2000/svg" width="14" height="18" viewBox="0 0 14 18" fill="none">
								<path d="M13 7.40121C13 4.01322 10.3137 1 7 1C3.68629 1 1 4.01322 1 7.40121V7.83938C0.999993 11.8962 4.57161 15.3412 6.20358 16.7036C6.67702 17.0988 7.32298 17.0988 7.79642 16.7036C9.42839 15.3412 13 11.8962 13 7.83938V7.40121Z" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
								<path d="M9.25 7.4012C9.25 8.72694 8.24264 9.80166 7 9.80166C5.75736 9.80166 4.75 8.72694 4.75 7.4012C4.75 6.07547 5.75736 5.00075 7 5.00075C8.24264 5.00075 9.25 6.07547 9.25 7.4012Z" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
							{{ oct_our_address }}
						</div>
						<div class="pr-footer-item-text grey-text fsz-14 lh-sm">{{ main_address }}</div>
					</div>
				{% endif %}
				{% if oct_contact_telephones is defined and oct_contact_telephones is not empty %}
					<div class="pr-footer-item pr-footer-item-phone d-inline-block mt-4 pr-dropdown-box position-relative">
						<div class="pr-footer-item-title white-text fw-700 d-inline-flex align-items-center gap-2 pr-dropdown-toggle">
							<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
								<path d="M5.93034 5.01489C6.37092 4.57431 6.48015 3.90123 6.2015 3.34394L5.43561 1.81216C5.10679 1.15451 4.33718 0.847842 3.64618 1.09912L1.93037 1.72305C1.3106 1.94842 0.91569 2.56694 1.01535 3.21884C2.02147 9.80018 6.21154 13.9902 12.7929 14.9964C13.4448 15.096 14.0633 14.7011 14.2887 14.0814L14.9126 12.3655C15.1639 11.6745 14.8572 10.9049 14.1996 10.5761L12.6678 9.81022C12.1105 9.53157 11.4374 9.64079 10.9968 10.0814L9.92513 11.1531C7.75378 10.4293 5.58243 8.25794 4.85864 6.08659L5.93034 5.01489Z" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
							{{ oct_contact_telephones[0] }}
							<svg class="pr-arrow-down" xmlns="http://www.w3.org/2000/svg" width="8" height="5" viewBox="0 0 8 5" fill="none">
								<path d="M1 0.511719L4 3.51172L7 0.511719" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
						</div>
						<div class="pr-dropdown p-3 content-block w-100 start-0 end-0">
							<ul class="list-unstyled ps-0 mb-0">
								{% for oct_contact_telephone in oct_contact_telephones %}
									<li{% if not loop.last %} class="mb-2"{% endif %}><a href="tel:{{ oct_contact_telephone|replace({" " : "", "-" : "", "(" : "", ")" : ""}) }}" class="light-link">{{ oct_contact_telephone }}</a></li>
								{% endfor %}
							</ul>
						</div>
					</div>
				{% endif %}
				{% if oct_promo_data.contact_email is defined and oct_promo_data.contact_email %}
					<div class="pr-footer-item pr-footer-item-mail mt-4">
						<div class="pr-footer-item-link fw-500 d-flex align-items-center gap-2">
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="14" viewBox="0 0 18 14" fill="none">
								<path d="M17 3.01133V11.0113C17 12.3368 15.9255 13.4113 14.6 13.4113H3.4C2.07452 13.4113 1 12.3368 1 11.0113V3.01133M17 3.01133C17 1.68584 15.9255 0.611328 14.6 0.611328H3.4C2.07452 0.611328 1 1.68584 1 3.01133M17 3.01133V3.26529C17 3.8488 16.6823 4.38604 16.1711 4.66724L10.1566 7.9752C9.43641 8.3713 8.56359 8.3713 7.8434 7.9752L1.82893 4.66724C1.31765 4.38604 1 3.8488 1 3.26529V3.01133" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
							<a href="mailto:{{ oct_promo_data.contact_email }}" class="light-link lh-sm">{{ oct_promo_data.contact_email }}</a>
						</div>
					</div>
				{% endif %}
				{% if oct_contact_opens is defined and oct_contact_opens is not empty %}
					<div class="pr-footer-item pr-footer-item-shedule mt-4">
						<div class="pr-footer-item-title white-text fw-700 d-flex align-items-center gap-2 mb-2">
							<svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" viewBox="0 0 18 18" fill="none">
								<path d="M9.0002 6.21172V9.01172L10.6001 10.6116M17 9.01172C17 13.43 13.4183 17.0117 9 17.0117C4.58172 17.0117 1 13.43 1 9.01172C1 4.59344 4.58172 1.01172 9 1.01172C13.4183 1.01172 17 4.59344 17 9.01172Z" stroke="#FFFDFF" stroke-linecap="round" stroke-linejoin="round"></path>
							</svg>
							{{ oct_working_hours}}
						</div>
						{% for oct_contact_open in oct_contact_opens %}
							<div class="pr-footer-item-text grey-text fsz-14">{{ oct_contact_open }}</div>
						{% endfor %}
					</div>
				{% endif %}
			</div>
			<div class="col-12 col-sm-6 col-lg-4">
				<div class="pr-footer-item-links d-flex gap-4">
					{% if categories is defined and categories %}
						<div class="pr-footer-item white-text">
							<div class="pr-footer-item-title fw-600 d-flex align-items-center gap-2 mb-2">{{ oct_footer_category }}</div>
							<ul class="pr-footer-item-list fsz-12 list-unstyled">
								{% for category in categories %}
									<li class="mb-1">
										<a href="{{ category.href }}" class="white-link link-sm" title="{{ category.name }}">{{ category.name }}</a>
									</li>
								{% endfor %}
							</ul>
						</div>
					{% endif %}
					{% if informations %}
						<div class="pr-footer-item white-text">
							<div class="pr-footer-item-title fw-600 d-flex align-items-center gap-2 mb-2">{{ text_information }}</div>
							<ul class="pr-footer-item-list fsz-12 list-unstyled">
								{% for information in informations %}
									<li class="mb-1">
										<a href="{{ information.href }}" class="white-link link-sm" title="{{ information.title }}">{{ information.title }}</a>
									</li>
								{% endfor %}
							</ul>
						</div>
					{% endif %}
                </div>
				<div class="pr-footer-item-payments d-flex flex-wrap gap-2 mt-4">				
