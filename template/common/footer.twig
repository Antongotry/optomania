					{% for payment_customer in oct_customer_payments %}
					<div class="pr-footer-item-payments-item br-4 bg-grey p-2 pr-footer-payments-item-custom">
						<img class="d-block img-fluid" src="{{ payment_customer }}" alt="Payment icon" width="50" height="30" loading="lazy">
					</div>
					{% endfor %}
                </div>
			</div>
