							<img class="d-block img-fluid" src="{{ base}}catalog/view/theme/oct_promo/images/payments/liqpay.svg" alt="LiqPay" width="50" height="30" loading="lazy">
						</div>
					{% endif %}
					{% if oct_promo_data.payments.visa is defined and oct_promo_data.payments.visa %}
