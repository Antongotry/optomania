{{ header }}
<div id="error-not-found" class="container-xl flex-grow-1 d-flex flex-column">
	<div class="content-top-box">{{ content_top }}</div>
	<div class="mt-3 p-lg-5 text-center h-100 flex-grow-1">
		<h1>{{ heading_title }}</h1>
		{% if (oct_404_image is defined and oct_404_image is not empty) %}
			<div class="mb-5"><img class="img-fluid" src="{{ oct_404_image }}" /></div>
		{% endif %}
		<div class="mt-3 dark-text fw-500">
			{{ text_error }}
		</div>
	</div>
	<div class="content-bottom-box">{{ content_bottom }}</div>
</div>
{{ footer }}
